{
  "name": "picto",
  "version": "1.0.0",
  "private": true,
  "description": "Modern icon management CLI tool",
  "author": "Picto Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky install",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ofetch": "^1.3.3",
    "ora": "^7.0.1",
    "prettier": "^3.2.4",
    "prompts": "^2.4.2",
    "svgo": "^3.2.0",
    "tsup": "^8.0.1",
    "turbo": "^1.11.0",
    "typescript": "^5.3.3",
    "zod": "^4.0.1"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.3.3",
      "tsup": "^8.0.1"
    }
  },
  "packageManager": "pnpm@8.15.0"
}
