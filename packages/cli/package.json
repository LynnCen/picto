{
  "name": "@picto/cli",
  "version": "0.0.1",
  "description": "Picto CLI tool",
  "type": "module",
  "bin": {
    "picto": "./bin/picto.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@picto/core": "workspace:*",
    "@picto/types": "workspace:*",
    "cac": "^6.7.14",
    "dotenv": "^16.4.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
